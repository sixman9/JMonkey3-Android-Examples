<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      android:versionCode="6"
      android:versionName="1.2.2" package="jmeproject.innovationtech.co.uk">
      
    <uses-sdk android:targetSdkVersion="8" android:minSdkVersion="8" />      
    
    <!-- Tell the system that you need ES 2.0. -->
    <uses-feature android:glEsVersion="0x00020000" android:required="true" />

    <!-- Tell the system that you need distinct touches (for the zoom gesture). -->
    <uses-feature android:name="android.hardware.touchscreen.multitouch.distinct" android:required="true" />

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name="EntryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
    <activity android:name="JMEProject1"><intent-filter><action android:name="android.intent.category.LAUNCHER"></action>
</intent-filter>
</activity>
        
    <activity android:name="JMEProject2"><intent-filter><action android:name="android.intent.category.LAUNCHER"></action>
</intent-filter>
</activity>

    <activity android:name="JMEProject3"><intent-filter><action android:name="android.intent.category.LAUNCHER"></action>
</intent-filter>
</activity>

</application>
</manifest>